>\n  // simulating async request\n  new Promise((resolve) => {\n    setTimeout(() => {\n      resolve([\n        {\n          id: 1,\n          name: 'James Bond'\n        },\n        {\n          id: 2,\n          name: 'Nikola Tesla'\n        }\n      ]);\n    }, 2000);\n  }).then(students => {\n    setState({\n      list: students\n    });\n  });\nexport default {\n  state: {\n    list: [],\n    loading: false,\n    error: undefined,\n    studentToAdd: ''\n  },\n  actions: {\n    removeStudent: studentId => ({ setState, getState }) => {\n      const { students } = getState();\n      setState({\n        list: students.list.filter(student => student.id !== studentId)\n      });\n    },\n    getStudents: asyncAction(getStudents),\n    changeStudentToAdd: value => ({ setState }) => {\n      setState({\n        studentToAdd: value\n      });\n    },\n    addStudent: () => ({ setState }) => {\n      setState(students => ({\n        list: students.list.concat({\n          id: students.list.length\n            ? students.list[students.list.length - 1].id + 1\n            : 1,\n          name: students.studentToAdd\n        }),\n        studentToAdd: ''\n      }));\n    }\n  }\n};\n"},"lineCount":null}},"error":null,"hash":"1841f561b5ed67822255fb42cb61f3bf","cacheData":{"env":{}}}